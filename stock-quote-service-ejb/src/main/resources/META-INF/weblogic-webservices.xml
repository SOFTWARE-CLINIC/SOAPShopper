<?xml version='1.0' encoding='UTF-8'?>
<weblogic-webservices
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://xmlns.oracle.com/weblogic/weblogic-webservices"
        xsi:schemaLocation="http://xmlns.oracle.com/weblogic/weblogic-webservices http://xmlns.oracle.com/weblogic/weblogic-webservices/1.1/weblogic-webservices.xsd">
    <webservice-description>
        <webservice-description-name>Stock Quote</webservice-description-name>
        <port-component>
            <port-component-name>StockQuotePort</port-component-name>
            <service-endpoint-address>
                <webservice-contextpath>/StockQuote</webservice-contextpath>
                <webservice-serviceuri>/StockQuote</webservice-serviceuri>
            </service-endpoint-address>
        </port-component>
    </webservice-description>
</weblogic-webservices>